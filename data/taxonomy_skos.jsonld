        {
            "@type": "skos:Concept",
            "@id": "treew:concept/21080401",
            "skos:notation": "21080401",
            "skos:prefLabel": [
                {
                    "@value": "Single vision lenses",
                    "@language": "en"
                },
                {
                    "@value": "Lentes monofocales",
                    "@language": "es"
                }
            ],
            "skos:inScheme": {
                "@id": "treew:scheme/classification-2025"
            },
            "skos:altLabel": [
                {
                    "@value": "Monofocal lenses",
                    "@language": "en"
                },
                {
                    "@value": "Regular lenses",
                    "@language": "en"
                },
                {
                    "@value": "Basic lenses",
                    "@language": "en"
                },
                {
                    "@value": "Lentes regulares",
                    "@language": "es"
                },
                {
                    "@value": "Lentes básicas",
                    "@language": "es"
                }
            ],
            "skos:definition": [
                {
                    "@value": "Single vision lenses are lenses that have a single prescription power throughout the entire lens to correct vision at a specific distance.",
                    "@language": "en"
                },
                {
                    "@value": "Las lentes monofocales son lentes que tienen un solo poder de prescripción en toda la lente para corregir la visión a una distancia específica.",
                    "@language": "es"
                }
            ],
            "skos:example": [
                {
                    "@value": "Standard single vision lenses",
                    "@language": "en"
                },
                {
                    "@value": "Clear single vision lenses",
                    "@language": "en"
                },
                {
                    "@value": "Lentes monofocales estándar",
                    "@language": "es"
                },
                {
                    "@value": "Lentes monofocales transparentes",
                    "@language": "es"
                }
            ],
            "skos:scopeNote": [
                {
                    "@value": "Single vision lenses are used for correcting vision at a single distance, such as for nearsightedness or farsightedness, and are not suitable for multifocal needs.",
                    "@language": "en"
                },
                {
                    "@value": "Las lentes monofocales se utilizan para corregir la visión a una sola distancia, como para la miopía o la hipermetropía, y no son adecuadas para necesidades multifocales.",
                    "@language": "es"
                }
            ],
            "skos:broader": {
                "@id": "treew:concept/210804"
            },
