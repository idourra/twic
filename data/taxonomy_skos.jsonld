        {
            "@type": "skos:Concept",
            "@id": "treew:concept/21080402",
            "skos:notation": "21080402",
            "skos:prefLabel": [
                {
                    "@value": "Bifocal glasses",
                    "@language": "en"
                },
                {
                    "@value": "Lentes bifocales",
                    "@language": "es"
                }
            ],
            "skos:inScheme": {
                "@id": "treew:scheme/classification-2025"
            },
            "skos:altLabel": [
                {
                    "@value": "Reading glasses",
                    "@language": "en"
                },
                {
                    "@value": "Presbyopic glasses",
                    "@language": "en"
                },
                {
                    "@value": "Multifocal glasses",
                    "@language": "en"
                },
                {
                    "@value": "Gafas de lectura",
                    "@language": "es"
                },
                {
                    "@value": "Gafas presbípicas",
                    "@language": "es"
                },
                {
                    "@value": "Gafas multifocales",
                    "@language": "es"
                }
            ],
            "skos:definition": [
                {
                    "@value": "Bifocal glasses are eyeglasses with lenses that have two distinct optical powers, typically used to correct presbyopia.",
                    "@language": "en"
                },
                {
                    "@value": "Las lentes bifocales son gafas con lentes que tienen dos potencias ópticas distintas, generalmente utilizadas para corregir la presbicia.",
                    "@language": "es"
                }
            ],
            "skos:example": [
                {
                    "@value": "Bifocal glasses with a clear upper lens and a magnifying lower lens",
                    "@language": "en"
                },
                {
                    "@value": "Bifocal sunglasses for outdoor reading",
                    "@language": "en"
                },
                {
                    "@value": "Lentes bifocales con un lente superior claro y un lente inferior de aumento",
                    "@language": "es"
                },
                {
                    "@value": "Gafas de sol bifocales para lectura al aire libre",
                    "@language": "es"
                }
            ],
            "skos:scopeNote": [
                {
                    "@value": "Bifocal glasses are specifically designed for individuals with presbyopia who require different optical powers for near and distant vision.",
                    "@language": "en"
                },
                {
                    "@value": "Las lentes bifocales están diseñadas específicamente para personas con presbicia que requieren diferentes potencias ópticas para la visión cercana y lejana.",
                    "@language": "es"
                }
            ],
            "skos:broader": {
                "@id": "treew:concept/210804"
            },
            "skos:related": [
                {
                    "@id": "treew:concept/21080401"
