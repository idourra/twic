            "skos:definition": [
                {
                    "@value": "Food category includes all edible items that can be consumed by humans, ranging from fresh produce to packaged goods, typically sold in grocery stores or online for consumption.",
                    "@language": "en"
                },
                {
                    "@value": "La categoría de alimentos incluye todos los productos comestibles que pueden ser consumidos por humanos, desde productos frescos hasta bienes envasados, generalmente vendidos en tiendas de comestibles o en línea para su consumo.",
                    "@language": "es"
                }
            ],
            "skos:example": [
                {
                    "@value": "Fresh fruits and vegetables",
                    "@language": "en"
                },
                {
                    "@value": "Canned soups and sauces",
